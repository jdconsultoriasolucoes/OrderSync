<input class="form-control" id="código_pet" name="código_pet" type="text"/>
</div>
<div class="mb-2">
<label class="form-label" for="nome_pet">Nome Pet:</label>
<input class="form-control" id="nome_pet" name="nome_pet" type="text"/>
</div>
<div class="subtitulo">Comissão Dispet:</div>
<div class="mb-2">
<label class="form-label" for="insumos">Insumos:</label>
<input class="form-control" id="insumos" name="insumos" type="text"/>
</div>
<div class="mb-2">
<label class="form-label" for="pet">Pet:</label>
<input class="form-control" id="pet" name="pet" type="text"/>
</div>
<div class="mb-2">
<label class="form-label" for="observações">Observações:</label>
<input class="form-control" id="observações" name="observações" type="text"/>
</div>
</div>
</div>
<div class="mt-4">
<button class="btn btn-warning" id="btn-editar" onclick="editarCliente()" type="button">Editar</button>
<button class="btn btn-secondary d-none" id="btn-cancelar" onclick="cancelarEdicao()" type="button">Cancelar</button>
<button class="btn btn-success d-none" id="btn-salvar" onclick="salvarCliente()" type="button">Salvar</button>
<button class="btn btn-danger" onclick="excluirCliente()" type="button">Excluir</button>
<button class="btn btn-primary" onclick="listarClientes()" type="button">Listar Clientes</button>
</div>
</form>
<script>
		 // Máscaras
    IMask(document.getElementById('cpf'), { mask: '000.000.000-00' });
    IMask(document.getElementById('cnpj'), { mask: '00.000.000/0000-00' });
    IMask(document.getElementById('telefone'), { mask: '(00) 0000-0000' });
    IMask(document.getElementById('celular'), { mask: '(00) 00000-0000' });

    function novoCliente() {
      document.getElementById("formCliente").reset();
      document.getElementById("id").value = "";
      document.getElementById("ativo").checked = true;
    }

    function salvarCliente() {
	  const id = document.getElementById("id").value;
      const cliente = obterDadosFormulario();
      
	  if (id) {
          cliente.id = parseInt(id);  // ⚠️ Garantir que vai como inteiro
	  }
	  const url = id ? `http://127.0.0.1:8000/cliente/${id}` : "http://127.0.0.1:8000/cliente";
      const metodo = id ? 'put' : 'post';
	  
      axios[metodo](url, cliente)
        .then(() => {
          alert("Cliente salvo com sucesso!");
          novoCliente();
        })
        .catch(erro => {
          console.error(erro);
          alert("Erro ao salvar cliente.");
        });
    }

    function excluirCliente() {
      const id = document.getElementById("id").value;
      if (!id) {
        alert("Selecione um cliente primeiro.");
        return;
      }

      if (!confirm("Tem certeza que deseja excluir este cliente?")) return;

      axios.delete(`http://127.0.0.1:8000/cliente/${id}`)
        .then(() => {
          alert("Cliente excluído com sucesso!");
          novoCliente();
        })
        .catch(erro => {
          console.error(erro);
          alert("Erro ao excluir cliente.");
        });
    }

    function listarClientes() {
      window.open("modal_clientes.html", "popup", "width=800,height=600");
    }

    function obterDadosFormulario() {
      return {
        tipo_cliente: document.getElementById("tipo_cliente").value,
        tipo_venda: document.getElementById("tipo_venda").value,
        tipo_compra: document.getElementById("tipo_compra").value,
        limite_credito: parseFloat(document.getElementById("limite_credito").value || 0),
        cnpj: document.getElementById("cnpj").value,
        inscricao_estadual: document.getElementById("inscricao_estadual").value,
        cpf: document.getElementById("cpf").value,
        nome: document.getElementById("nome").value,
        nome_fantasia: document.getElementById("nome_fantasia").value,
        telefone: document.getElementById("telefone").value,
        celular: document.getElementById("celular").value,
        email: document.getElementById("email").value,
        observacoes: document.getElementById("observacoes").value,
        ativo: document.getElementById("ativo").checked,
        endereco_entrega: {
          cep: document.getElementById("cep_entrega").value,
          endereco: document.getElementById("endereco_entrega").value,
          complemento: document.getElementById("complemento_entrega").value,
          bairro: document.getElementById("bairro_entrega").value,
          cidade: document.getElementById("cidade_entrega").value,
          estado: document.getElementById("estado_entrega").value,
        },
        endereco_cobranca: {
          cep: document.getElementById("cep_cobranca").value,
          endereco: document.getElementById("endereco_cobranca").value,
          complemento: document.getElementById("complemento_cobranca").value,
          bairro: document.getElementById("bairro_cobranca").value,
          cidade: document.getElementById("cidade_cobranca").value,
          estado: document.getElementById("estado_cobranca").value,
        }
      };
    }

    function preencherFormularioCliente(cliente) {
      document.getElementById("id").value = cliente.id || "";
      document.getElementById("tipo_cliente").value = cliente.tipo_cliente;
      document.getElementById("tipo_venda").value = cliente.tipo_venda;
      document.getElementById("tipo_compra").value = cliente.tipo_compra;
      document.getElementById("limite_credito").value = cliente.limite_credito;
      document.getElementById("cnpj").value = cliente.cnpj;
      document.getElementById("inscricao_estadual").value = cliente.inscricao_estadual;
      document.getElementById("cpf").value = cliente.cpf;
      document.getElementById("nome").value = cliente.nome;
      document.getElementById("nome_fantasia").value = cliente.nome_fantasia;
      document.getElementById("telefone").value = cliente.telefone;
      document.getElementById("celular").value = cliente.celular;
      document.getElementById("email").value = cliente.email;

      document.getElementById("cep_entrega").value = cliente.endereco_entrega?.cep || "";
      document.getElementById("endereco_entrega").value = cliente.endereco_entrega?.endereco || "";
      document.getElementById("complemento_entrega").value = cliente.endereco_entrega?.complemento || "";
      document.getElementById("bairro_entrega").value = cliente.endereco_entrega?.bairro || "";
      document.getElementById("cidade_entrega").value = cliente.endereco_entrega?.cidade || "";
      document.getElementById("estado_entrega").value = cliente.endereco_entrega?.estado || "";

      document.getElementById("cep_cobranca").value = cliente.endereco_cobranca?.cep || "";
      document.getElementById("endereco_cobranca").value = cliente.endereco_cobranca?.endereco || "";
      document.getElementById("complemento_cobranca").value = cliente.endereco_cobranca?.complemento || "";
      document.getElementById("bairro_cobranca").value = cliente.endereco_cobranca?.bairro || "";
      document.getElementById("cidade_cobranca").value = cliente.endereco_cobranca?.cidade || "";
      document.getElementById("estado_cobranca").value = cliente.endereco_cobranca?.estado || "";

      document.getElementById("observacoes").value = cliente.observacoes || "";
      document.getElementById("ativo").checked = cliente.ativo ?? true;
    }

    // Disponibiliza para o modal chamar
    window.preencherFormulario = preencherFormularioCliente;
	  <script src="js/script.js"></script>
</body>
</html>